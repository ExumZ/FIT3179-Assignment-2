{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A simple donut chart with embedded data.",
    "data": {
        "url": "https://raw.githubusercontent.com/ExumZ/FIT3179-Assignment-2/main/main_data/developmental_group_data_2000_2020_cleaned.csv"
    },
    "mark": {
        "type": "arc", 
        "innerRadius": 50
    },
    "encoding": {
        "theta": {
            "field": "value", 
            "type": "quantitative"
        },
        "color": {
            "field": "category", 
            "type": "nominal"
        }
    }
}